---
title: 'Indexed Pagination Test'
isPaged: true
pageOrder: 0
pagedCollection: 'documents'
pageSize: 2
---
<ul class="pager">
	<% if !@hasNextPage(): %>
		<li class="previous disabled"><span>&larr; Older</span></li>
	<% else: %>
		<li class="previous"><a href="<%= @getNextPage() %>">&larr; Older</a></li>
	<% end %>
	<% if !@hasPrevPage(): %>
		<li class="next disabled"><span>Newer &rarr;</span></li>
	<% else: %>
		<li class="next"><a href="<%= @getPrevPage() %>">Newer &rarr;</a></li>
	<% end %>
</ul>